<div class="container">
  <h1>Documentos Subidos</h1>
  <div class="row">
    <div class="col-md-12">
      <table class="table table-striped table-hover ">
        <thead>
        <tr>
          <th>#</th>
          <th>T&iacute;tulo</th>
          <th>Fecha</th>
          <th>Usuario</th>
          <th>Abstract</th>
          <th>Label</th>
          <th></th>
        </tr>
        </thead>
        <tbody>
        <tr ng-repeat="record in $ctrl.tableData">
          <td ng-bind="record.id"></td>
          <td><span class="text-warning" ng-bind="record.createdAt | date:'mediumDate'"></span></td>
          <td ng-bind="record.title"></td>
          <td><span class="label label-info" ng-bind="record.idUser.userName"></span></td>
          <td ng-bind="record.abstract"></td>
          <td ng-bind="record.label"></td>
          <td><span class="btn btn-success">Ver Adjuntos</span></td>
        </tr>
        </tbody>
      </table>
    </div>
  </div>



</div>
